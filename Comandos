2.
CREATE TABLESPACE gofar_travel DATAFILE 
'C:/ORACLEXE/app/oracle/oradata/XE/gofar_travel01.dbf' SIZE 500M, 
'C:/ORACLEXE/app/oracle/oradata/XE/gofar_travel02.dbf' SIZE 250M,
'C:/ORACLEXE/app/oracle/oradata/XE/gofar_travel03.dbf' SIZE 250M;

CREATE TABLESPACE test_purposes DATAFILE 
'C:/ORACLEXE/app/oracle/oradata/XE/test_purposes01.dbf' SIZE 500M
 EXTENT MANAGEMENT LOCAL AUTOALLOCATE;

CREATE UNDO TABLESPACE UNDOTBS2 DATAFILE 
'C:/ORACLEXE/app/oracle/oradata/XE/UNDOTBS2_01.dbf' SIZE 5M;

3.
ALTER SYSTEM SET UNDO_TABLESPACE=UNDOTBS2 SCOPE=BOTH;

4.
CREATE USER ADMINISTRADOR
IDENTIFIED BY adm
DEFAULT TABLESPACE gofar_travel
QUOTA UNLIMITED ON gofar_travel
ACCOUNT UNLOCK;

5.
GRANT DBA TO ADMINISTRADOR;
GRANT CREATE SESSION TO ADMINISTRADOR;

6.
CREATE PROFILE manager1 LIMIT
SESSIONS_PER_USER 1
PASSWORD_LIFE_TIME 40
IDLE_TIME 15
FAILED_LOGIN_ATTEMPTS 4
CPU_PER_SESSION UNLIMITED
CPU_PER_CALL UNLIMITED;

CREATE PROFILE finance LIMIT
SESSIONS_PER_USER 1
PASSWORD_LIFE_TIME 15
IDLE_TIME 3
FAILED_LOGIN_ATTEMPTS 2
CPU_PER_SESSION UNLIMITED
CPU_PER_CALL UNLIMITED;

CREATE PROFILE development LIMIT
SESSIONS_PER_USER 2
PASSWORD_LIFE_TIME 100
IDLE_TIME 30
FAILED_LOGIN_ATTEMPTS UNLIMITED
CPU_PER_SESSION UNLIMITED
CPU_PER_CALL UNLIMITED;

7.
CREATE USER USER1
IDENTIFIED BY USER1
DEFAULT TABLESPACE gofar_travel
QUOTA UNLIMITED ON gofar_travel
PROFILE manager1 
ACCOUNT UNLOCK;

GRANT CREATE SESSION TO USER1;

CREATE USER USERMANAGER
IDENTIFIED BY USERMANAGER
DEFAULT TABLESPACE gofar_travel
QUOTA UNLIMITED ON gofar_travel
PROFILE manager1 
ACCOUNT UNLOCK;

GRANT CREATE SESSION TO USERMANAGER;

CREATE USER USERFINANCE
IDENTIFIED BY USERFINANCE
DEFAULT TABLESPACE gofar_travel
QUOTA UNLIMITED ON gofar_travel
PROFILE finance  
ACCOUNT UNLOCK;

GRANT CREATE SESSION TO USERFINANCE;

CREATE USER USERDEVELOPMENT
IDENTIFIED BY USERDEVELOPMENT
DEFAULT TABLESPACE gofar_travel
QUOTA UNLIMITED ON gofar_travel
PROFILE development  
ACCOUNT UNLOCK;

GRANT CREATE SESSION TO USERDEVELOPMENT;

8.
ALTER USER USERFINANCE ACCOUNT LOCK;
ALTER USER USER1 ACCOUNT LOCK;
ALTER USER USERMANAGER ACCOUNT LOCK;

9.
DROP TABLESPACE TEST_PURPOSES INCLUDING CONTENTS AND DATAFILES;

10.CREATE TABLE VEHICLETYPE
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
Name varchar(20) NOT NULL
);
CREATE SEQUENCE VEHICLETYPE_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE TABLE VEHICLE
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
VIN varchar(50) CONSTRAINT VIN  UNIQUE NOT NULL,
VEHICLETYPE_ID NUMBER(19,0) NOT NULL, 
NAME VARCHAR(200) NOT NULL,
MODEL NUMBER(5) NOT NULL,
YEAR NUMBER(5) NOT NULL,
MANUFACTURENAME VARCHAR(200),
COST NUMBER(20,4),
FOREIGN KEY (VEHICLETYPE_ID)
REFERENCES VEHICLETYPE(ID)
);
CREATE SEQUENCE VEHICLE_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE TABLE TRADEIN
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
VEHICLE_ID NUMBER(19,0) NOT NULL, 
SERIALNUMBER varchar(50) CONSTRAINT SERIALNUMBER  UNIQUE NOT NULL,
MAKE VARCHAR(200) NOT NULL,
MODEL NUMBER(5) NOT NULL,
YEAR NUMBER(5) NOT NULL,
AMOUNT NUMBER(20,4),
FOREIGN KEY (VEHICLE_ID)
REFERENCES VEHICLE(ID)
);
CREATE SEQUENCE TRADEIN_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE TABLE CUSTOMER
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
NAME VARCHAR(50) NOT NULL,
ADDRES VARCHAR(50) NOT NULL,
PHONENUMBER NUMBER(20,0) NOT NULL
);
CREATE SEQUENCE CUSTOMER_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE TABLE SALESPERSON
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
Name varchar(20) NOT NULL
);
CREATE SEQUENCE SALESPERSON_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE TABLE CUSTOMER_SALESPERSON
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
CUSTOMER_ID NUMBER(19,0) NOT NULL,
SALESPERSON_ID NUMBER(19,0) NOT NULL,
FOREIGN KEY (CUSTOMER_ID)
REFERENCES CUSTOMER(ID),
FOREIGN KEY (SALESPERSON_ID)
REFERENCES SALESPERSON(ID)
);
CREATE SEQUENCE CUSTOMER_SALESPERSON_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE TABLE INVOICEHEAD
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
CUSTOMER_SALESPERSON_ID NUMBER(19,0) NOT NULL,
TOTALAMOUNT NUMBER(20,4) NOT NULL,
TAXTOTAL NUMBER(20,4) NOT NULL,
FOREIGN KEY (CUSTOMER_SALESPERSON_ID)
REFERENCES CUSTOMER_SALESPERSON(ID)
);
CREATE SEQUENCE INVOICEHEAD_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE TABLE INVOICEDTL
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
INVOICEHEAD_ID NUMBER(19,0) NOT NULL,
VEHICLE_ID NUMBER(19,0) NOT NULL,
LINE NUMBER(10,0) NOT NULL,
QTY NUMBER(20,4) NOT NULL,
UNITPRICE NUMBER(20,4) NOT NULL,
EXTPRICE NUMBER(20,4) NOT NULL,
FOREIGN KEY (INVOICEHEAD_ID)
REFERENCES INVOICEHEAD(ID),
FOREIGN KEY (VEHICLE_ID)
REFERENCES VEHICLE(ID)
);
CREATE SEQUENCE INVOICEDTL_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE TABLE WHAREHOUSE
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
Name varchar(20) NOT NULL
);
CREATE SEQUENCE WHAREHOUSE_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE TABLE PURCHASEOPTION
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
Name varchar(20) NOT NULL,
UNITPRICE NUMERIC(20,4)
);
CREATE SEQUENCE PURCHASEOPTION_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE TABLE INVENTORY
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
WHAREHOUSE_ID NUMBER(19,0) NOT NULL, 
PURCHASEOPTION_ID NUMBER(19,0) NOT NULL, 
ONHANQTY NUMBER(20,4) NOT NULL,
FOREIGN KEY (WHAREHOUSE_ID)
REFERENCES WHAREHOUSE(ID),
FOREIGN KEY (PURCHASEOPTION_ID)
REFERENCES PURCHASEOPTION(ID)
);
CREATE SEQUENCE INVENTORY_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE TABLE INVOICEDTL_PURCHASEOPTION
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
INVOICEDTL_ID NUMBER(19,0) NOT NULL, 
PURCHASEOPTION_ID NUMBER(19,0) NOT NULL, 
QTY NUMBER(20,5) NOT NULL,
FOREIGN KEY (INVOICEDTL_ID)
REFERENCES INVOICEDTL(ID),
FOREIGN KEY (PURCHASEOPTION_ID)
REFERENCES PURCHASEOPTION(ID)
);
CREATE SEQUENCE INVOICEDTL_PURCHASEOPTION_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE TABLE INSTALLSERVICE
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
DESCRIPTION varchar(20) NOT NULL,
UNITCOST NUMBER(20,5) NOT NULL,
UNITPRICE NUMBER(20,5) NOT NULL
);
CREATE SEQUENCE INSTALLSERVICE_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE TABLE INVOICEDTL_INSTALLSERVICE
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
INVOICEDTL_ID NUMBER(19,0) NOT NULL, 
INSTALLSERVICE_ID NUMBER(19,0) NOT NULL, 
FOREIGN KEY (INVOICEDTL_ID)
REFERENCES INVOICEDTL(ID),
FOREIGN KEY (INSTALLSERVICE_ID)
REFERENCES INSTALLSERVICE(ID)
);
CREATE SEQUENCE INVOICEDTL_INSTALLSERVICE_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE TABLE TAX
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
NAME VARCHAR (50) NOT NULL,
PERCENT NUMBER(2,5) NOT NULL
);
CREATE SEQUENCE TAX_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE TABLE INVOICEDTL_TAX
(
ID NUMBER(19,0) PRIMARY KEY NOT NULL,
INVOICEDTL_ID NUMBER(19,0) NOT NULL, 
TAX_ID NUMBER(19,0) NOT NULL, 
FOREIGN KEY (INVOICEDTL_ID)
REFERENCES INVOICEDTL(ID),
FOREIGN KEY (TAX_ID)
REFERENCES TAX(ID)
);
CREATE SEQUENCE INVOICEDTL_TAX_SEQ
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;


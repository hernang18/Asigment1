INSERT INTO INVOICEDTL(ID,INVOICEHEAD_ID,VEHICLE_ID,LINE,QTY,UNITPRICE,EXTPRICE) 
SELECT INVOICEDTL_SEQ.NEXTVAL,1,1,1,1,COST,COST*1 FROM VEHICLE WHERE ID=1;
INSERT INTO INVOICEDTL_TAX (ID,INVOICEDTL,TAX_ID) SELECT INVOICEDTL_TAX.NEXTVAL,1,PERCENT FROM TAX WHERE NAME='IVA 19';
UPDATE INVOICEDTL SET EXTPRICE=EXTPRICE*(1+(SELECT PERCENT FROM TAX WHERE NAME='IVA 19')/100) WHERE ID=1;
UPDATE INVOICEHEAD SET TOTALAMOUNT=(SELECT SUM(EXTPRICE*QTY) FROM INVOICEDTL WHERE ID=2),
                       TAXTOTAL=(SELECT SUM(EXTPRICE-UNITPRICE) FROM INVOICEDTL WHERE ID=2) WHERE ID=1;
                       
